<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
  <class name="ZLERP.Model.RoleOrganization, ZLERP.Model" table="RoleOrganization" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    <id name="ID" column="RoleOrganizationID">
      <generator class="identity" />
    </id>
    <property name="RoleID" column="RoleID" type="string"  />
    <property name="OrganizationID" column="OrganizationID" type="int"  />
    
    <many-to-one name="Role">
      <column name="RoleID" sql-type="nvarchar" not-null="false" />
    </many-to-one>

    <many-to-one name="Organization">
      <column name="OrganizationID" sql-type="nvarchar" not-null="false" />
    </many-to-one>


  </class>
</hibernate-mapping>