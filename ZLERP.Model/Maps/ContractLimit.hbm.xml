<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
  <class name="ZLERP.Model.ContractLimit, ZLERP.Model" table="ContractLimit" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    <id name="ID" column="ContractLimitID">
      <generator class="native">
      </generator>
    </id>
    <version name="Version" column="Version" />
    <property name="ContractID" column="ContractID" />
    <property name="IsEnabled" column="IsEnabled" />
    <property name="LimitType" column="LimitType" />
    <property name="WarnValue" column="WarnValue" />
    <property name="LimitValue" column="LimitValue" />

    <property name="Relation" column="Relation" />
    <property name="IsWarn" column="IsWarn" />
    <property name="IsLimit" column="IsLimit" />
    <property name="CurrentValue" column="CurrentValue" />
    
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />
    <property name="CheckWarnMessage" column="CheckWarnMessage" />
    <property name="CheckLimitMessage" column="CheckLimitMessage" />
    
    <many-to-one name="Contract" column="ContractID" fetch="join" class="Contract" insert="false" update="false"/>  
  </class>
</hibernate-mapping>