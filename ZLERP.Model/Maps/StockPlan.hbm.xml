<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
	<class name="ZLERP.Model.StockPlan, ZLERP.Model" table="StockPlan" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ID">
      <generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="PlanNo" column="PlanNo" />
    <property name="StockPactID" column="StockPactID" />
    <property name="PlanDate" column="PlanDate" />
    <property name="ExecStatus" column="ExecStatus" />
    <property name="StuffID" column="StuffID" />
    <property name="QualityNeed" column="QualityNeed" />
    <property name="SourceAddr" column="SourceAddr" />
    <property name="PurNo" column="PurNo" />
    <property name="Linker" column="Linker" />
    <property name="LinkPhone" column="LinkPhone" />
    <property name="OrderNum" column="OrderNum" />
    <property name="TransID" column="TransID" />
    <property name="CarNo" column="CarNo" />
    <property name="CarLinker" column="CarLinker" />
    <property name="CarLinkPhone" column="CarLinkPhone" />
    <property name="CarDriver" column="CarDriver" />
    <property name="OutRate" column="OutRate" />
    <property name="SiloID" column="SiloID" />
    <property name="OneWeight" column="OneWeight" />
    <property name="PlanMan" column="PlanMan" />
    <property name="CheckMan" column="CheckMan" />
    <property name="CheckDate" column="CheckDate" />
    <property name="TransportMode" column="TransportMode" />
    <property name="PlanAmount" column="PlanAmount" /> 
    
		<property name="Auditor" column="Auditor" />
		<property name="AuditTime" column="AuditTime" />
		<property name="AuditStatus" column="AuditStatus" />
		<property name="AuditInfo" column="AuditInfo" /> 
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />
 
		<many-to-one name="StockPact" column="StockPactID" class="StockPact" update="false" insert="false" not-null="false" />
 
    <many-to-one name="TransInfo" column="TransID" class="SupplyInfo" insert="false" update="false" not-null="false" />
    <many-to-one name="StuffInfo" column="StuffID" class="StuffInfo" insert="false" update="false" not-null="false" />
    <many-to-one name="Silo" column="SiloID" class="Silo" insert="false" update="false" not-null="false" />

    <bag name="StuffIns" lazy="true" batch-size="30" inverse="true" >
      <key column="PlanNo"></key>
      <one-to-many class="StuffIn"></one-to-many>
    </bag>
  </class>
</hibernate-mapping>