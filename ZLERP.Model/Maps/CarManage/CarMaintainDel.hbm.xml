<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.CarManage" assembly="ZLERP.Model">
	<class name="ZLERP.Model.CarManage.CarMaintainDel, ZLERP.Model" table="CarMaintainDel" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ID">
      <generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="MaintainID" column="MaintainID" />
    <property name="ItemID" column="ItemID" />
    <property name="MaterialNum" column="MaterialNum" />
    <property name="MaterialPrice" column="MaterialPrice" />
    <property name="MaterialCost" column="MaterialCost" />
    <property name="ManHour" column="ManHour" />
    <property name="ManHourCost" column="ManHourCost" />  
    <property name="AllCost" column="AllCost" /> 
		<property name="Remark" column="Remark" /> 
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    <many-to-one name="CarMaintainItem"  not-found="ignore">
      <column name="ItemID" sql-type="nvarchar" not-null="false"  />
    </many-to-one>
    
  </class>
</hibernate-mapping>