<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.Beton" assembly="ZLERP.Model">
  <class name="ZLERP.Model.Beton.B_CarFleet, ZLERP.Model" table="B_CarFleet" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    <id name="ID" column="ID">
      <generator class="native" />
    </id>
    <version name="Version" column="Version" />
    <property name="FleetCode" column="FleetCode" />
    <property name="FleetName" column="FleetName" />
    <property name="FleetType" column="FleetType" />
    <property name="Adrress" column="Adrress" />
    <property name="Linker" column="Linker" /> 
    <property name="LinkPhone" column="LinkPhone" />
    <property name="AccountNo" column="AccountNo" />
    <property name="TaxNo" column="TaxNo" />
    <property name="PaidIn" column="PaidIn" />
    <property name="PaidOut" column="PaidOut" />
    <property name="PaidOwing" column="PaidOwing" />
    <property name="PaidFavourable" column="PaidFavourable" />
    <property name="PayMoney" column="PayMoney" />
    <property name="PrepayInit" column="PrepayInit" />
    <property name="PrePay" column="PrePay" />
    <property name="CarTemplet" column="CarTemplet" />
    <property name="IsStop" column="IsStop" />
    <property name="Remark" column="Remark" /> 
    
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />


    <many-to-one name="B_FareTemplet"  column="CarTemplet" class="ZLERP.Model.Beton.B_FareTemplet" update="false" insert="false" />
  </class>


</hibernate-mapping>