<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
	<class name="ZLERP.Model.ContractItem, ZLERP.Model" table="ContractItems" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ContractItemsID">
		 	<generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="ConStrength" column="ConStrength" />
		<property name="UnPumpPrice" column="UnPumpPrice" />
		<property name="PumpCost" column="PumpCost" />
    <property name="SlurryPrice" column="SlurryPrice" />
    <property name="BaseConStrength" column="BaseConStrength" />
    <property name="ExMoney" column="ExMoney" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />
    <property name="ContractID" column="ContractID" />
    <property name="ConStrengthID" column="ConStrengthID" />
    <property name="NewPriceDate" column="NewPriceDate" />
    <property name="Auditor" column="Auditor" />
    <property name="AuditTime" column="AuditTime" />
    <property name="AuditStatus" column="AuditStatus" />
    <property name="AuditInfo" column="AuditInfo" />
    <property name="Auditor2" column="Auditor2" />
    <property name="AuditTime2" column="AuditTime2" />
    <property name="AuditStatus2" column="AuditStatus2" />
    <property name="AuditInfo2" column="AuditInfo2" />
    <many-to-one name="Contract" column="ContractID" class="Contract" update="false" insert="false" />
    <many-to-one name="ConStrengthClass" column="ConStrengthID" class="ConStrength" update="false" insert="false" not-found="ignore" />
		<bag name="IdentitySettings" lazy="true"  inverse="true" >
			<key column="ContractItemsID"></key>
			<one-to-many class="IdentitySetting"></one-to-many>
		</bag>
    <bag name="ProduceTasks" lazy="true" inverse="true" >
      <key column="ContractItemsID"></key>
      <one-to-many class="ProduceTask"></one-to-many>
    </bag>
	</class>
</hibernate-mapping>