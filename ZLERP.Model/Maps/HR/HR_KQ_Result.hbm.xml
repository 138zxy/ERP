<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.HR" assembly="ZLERP.Model">
  <class name="ZLERP.Model.HR.HR_KQ_Result, ZLERP.Model" table="HR_KQ_Result" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    <id name="ID" column="ID">
      <generator class="native" />
    </id>
    <version name="Version" column="Version" /> 
    <property name="PersonID" column="PersonID" />
    <property name="WeekShow" column="WeekShow" />
    <property name="WorkDate" column="WorkDate" />
    <property name="LateMinute" column="LateMinute" />
    <property name="LeaveMinute" column="LeaveMinute" />
    <property name="OnleaveHour" column="OnleaveHour" />
    <property name="OverHour" column="OverHour" />
    <property name="TravelHour" column="TravelHour" />
    <property name="ChangeHour" column="ChangeHour" />
    <property name="IsAway" column="IsAway" />
    <property name="AwayHour" column="AwayHour" />
    <property name="IsUnusual" column="IsUnusual" />
    <property name="IsAllOver" column="IsAllOver" />
    <property name="WorkStartTime" column="WorkStartTime" />
    <property name="WorkEndTime" column="WorkEndTime" />
    <property name="UpStartTime" column="UpStartTime" />
    <property name="DwonEndTime" column="DwonEndTime" />
    <property name="Remark" column="Remark" />
    <property name="YearMonth" column="YearMonth" />
    <property name="CheckUpTime" column="CheckUpTime" />
    <property name="CheckDownTime" column="CheckDownTime" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    <many-to-one name="HR_Base_Personnel" not-found="ignore">
      <column name="PersonID" sql-type="nvarchar" not-null="false"  />
    </many-to-one>
    
  </class>


</hibernate-mapping>