<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
  <class name="ZLERP.Model.Lab_Template, ZLERP.Model" table="Lab_Template" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    <id name="ID" column="LabTemplateID">
      <generator class="ZLERP.Id.TablePrefixGenerator,ZLERP.Id" />
    </id>
    <version name="Version" column="Version" />
    <property name="LabTemplateName" column="LabTemplateName" />
    <property name="LabTemplateType" column="LabTemplateType" />
    <property name="LabTemplatePath" column="LabTemplatePath" />
    <property name="DocType" column="DocType" />
    <property name="Meno" column="Meno" />
    
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    <many-to-one name="Dic" column="LabTemplateType" class="Dic" update="false" insert="false"  not-found="ignore"/>
  </class>
</hibernate-mapping>