<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.SupplyChain" assembly="ZLERP.Model">
	<class name="ZLERP.Model.SupplyChain.SC_PiaoIn, ZLERP.Model" table="SC_PiaoIn" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ID">
      <generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="SupplierID" column="SupplierID" />
    <property name="LibID" column="LibID" />
    <property name="InType" column="InType" />
    <property name="InDate" column="InDate" />
    <property name="InNo" column="InNo" />
    <property name="Purchase" column="Purchase" />
    <property name="VarietyNum" column="VarietyNum" />
    <property name="InMoney" column="InMoney" />
    <property name="PayType" column="PayType" />
    <property name="PayMoney" column="PayMoney" />
    <property name="PayOwing" column="PayOwing" />
    <property name="PayFavourable" column="PayFavourable" />
    <property name="Condition" column="Condition" />
    <property name="PiaoNo" column="PiaoNo" />
    <property name="Evaluate" column="Evaluate" />
    <property name="PurchaseID" column="PurchaseID" />
    <property name="AuditTime" column="AuditTime" />
    <property name="Auditor" column="Auditor" />
		<property name="Remark" column="Remark" />
        
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    
    <many-to-one name="SC_Supply" not-found="ignore">
      <column name="SupplierID" sql-type="nvarchar" not-null="false"  />
    </many-to-one>
    <many-to-one name="SC_Lib"  not-found="ignore">
      <column name="LibID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
    <bag name="SC_ZhangIns" lazy="true" batch-size="30" inverse="true" >
      <key column="InNo"></key>
      <one-to-many class="SC_ZhangIn"></one-to-many>
    </bag>
    <many-to-one name="SC_PiaoInOrder"  not-found="ignore">
      <column name="PurchaseID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
  </class>
</hibernate-mapping>