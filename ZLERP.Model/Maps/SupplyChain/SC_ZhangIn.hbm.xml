    <property name="Sequence" column="Sequence" />
    <property name="Indate" column="Indate" />
    <property name="Quantity" column="Quantity" />
    <property name="PriceUnit" column="PriceUnit" />
    <property name="InMoney" column="InMoney" />
    <property name="PiNo" column="PiNo" /> 
    <property name="Remark" column="Remark" />
    <property name="Unit" column="Unit" />
