<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.SupplyChain" assembly="ZLERP.Model">
	<class name="ZLERP.Model.SupplyChain.SC_Fixed, ZLERP.Model" table="SC_Fixed" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ID">
      <generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="Fcode" column="Fcode" />
    <property name="Fname" column="Fname" />
    <property name="Ftype" column="Ftype" />
    <property name="Spec" column="Spec" />
    <property name="Manufacturer" column="Manufacturer" />
    <property name="Supply" column="Supply" />
    <property name="InterCode" column="InterCode" />
    <property name="Configure" column="Configure" />
    <property name="ProductDate" column="ProductDate" />
    <property name="AddType" column="AddType" />
    <property name="AddDate" column="AddDate" />
    <property name="FixedPirce" column="FixedPirce" />
    <property name="Position" column="Position" />
    <property name="DepartMent" column="DepartMent" />
    <property name="Storeman" column="Storeman" />
    <property name="NetSalvageRate" column="NetSalvageRate" />
    <property name="NetSalvage" column="NetSalvage" />
    <property name="Depreciation" column="Depreciation" />
    <property name="Remark" column="Remark" />
    <property name="Condition" column="Condition" />
    <property name="DepreciaMonth" column="DepreciaMonth" />
    <property name="DepreciaYear" column="DepreciaYear" />
    <property name="DepreciaAll" column="DepreciaAll" />
    <property name="NetWorth" column="NetWorth" />
    <property name="BorrowDepart" column="BorrowDepart" />
    <property name="borrowMan" column="borrowMan" />
    <property name="ClearDate" column="ClearDate" />
    <property name="BarCode" column="BarCode" />
    <property name="QRCode" column="QRCode" />
    <property name="BrevityCode" column="BrevityCode" />
    <property name="UseYear" column="UseYear" />
    <property name="UseEndDate" column="UseEndDate" />
    <property name="PicUrl" column="PicUrl" />
    <property name="StoreDate" column="StoreDate" />
    <property name="DepreciaMonthRate" column="DepreciaMonthRate" />
    <property name="DepreciaMonthMoney" column="DepreciaMonthMoney" /> 
    
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    <many-to-one name="SC_GoodsType">
      <column name="FType" sql-type="nvarchar" not-null="false" />
    </many-to-one>

    
    <bag name="Attachments" where="ObjectType='SC_Fixed'" batch-size="30" cascade="delete" >
      <key column="ObjectId"/>
      <one-to-many class="ZLERP.Model.Attachment" />
    </bag>
	</class>
</hibernate-mapping>