<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.SupplyChain" assembly="ZLERP.Model">
	<class name="ZLERP.Model.SupplyChain.SC_PiaoOut, ZLERP.Model" table="SC_PiaoOut" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ID">
      <generator class="native" />
		</id>
		<version name="Version" column="Version" />
    <property name="UserID" column="UserID" />
    <property name="DepartmentID" column="DepartmentID" />
    <property name="LibID" column="LibID" />
    <property name="OutType" column="OutType" /> 
    <property name="OutDate" column="OutDate" />
    <property name="OutNo" column="OutNo" /> 
    <property name="VarietyNum" column="VarietyNum" />
    <property name="OutMoney" column="OutMoney" />
    <property name="PayType" column="PayType" />
    <property name="PayMoney" column="PayMoney" />
    <property name="PayOwing" column="PayOwing" />
    <property name="PayFavourable" column="PayFavourable" />
    <property name="Condition" column="Condition" />
    <property name="PiaoNo" column="PiaoNo" />
    <property name="Evaluate" column="Evaluate" />
    <property name="OutOrderID" column="OutOrderID" />
    <property name="AuditTime" column="AuditTime" />
    <property name="Auditor" column="Auditor" />
		<property name="Remark" column="Remark" />
    <property name="MaintainID" column="MaintainID" />
    <property name="CarMaintainID" column="CarMaintainID" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />
    <many-to-one name="SC_PiaoOutOrder"  not-found="ignore">
      <column name="OutOrderID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
    <many-to-one name="SC_Lib">
      <column name="LibID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
    <bag name="SC_ZhangOuts" lazy="true" batch-size="30" inverse="true" >
      <key column="OutNo"></key>
      <one-to-many class="SC_ZhangOut"></one-to-many>
    </bag>
    <many-to-one name="Department">
      <column name="DepartmentID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
    <many-to-one name="SC_Fixed_Maintain"  not-found="ignore">
      <column name="MaintainID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
    <many-to-one name="CarMaintain"  not-found="ignore">
      <column name="CarMaintainID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
  </class>
</hibernate-mapping>