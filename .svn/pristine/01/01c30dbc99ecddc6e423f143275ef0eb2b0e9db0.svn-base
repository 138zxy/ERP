@model ZLERP.Model.Lab_Record 
@section HeaderSection{
<script src="@Url.Content("~/Scripts/Views/Lab_Record/Lab_Record.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        $(function () {
            lab_recordIndexInit('@Url.Action("Find")');
        });
</script>
}
<div id="MyJqGrid"></div>

<div id="MyFormDiv" class="mvcform" style="display:none;">
@using (Html.BeginForm())
{@Html.ValidationSummary()
 <table class="mvcform">
            @Html.HiddenFor(m => m.ID)
            <tr>
                <td>@Html.LabelFor(m => m.ShowpeieNo)@Html.EditorFor(m => m.ShowpeieNo)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.Carno)@Html.EditorFor(m => m.Carno)
                </td>
                <td>@Html.LabelFor(m => m.Date)@Html.EditorFor(m => m.Date, "DateTimeWithTime", new { defaultValue = @DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss") })
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.Siloid)@Html.EditorFor(m => m.Siloid)
                </td>
                <td>@Html.LabelFor(m => m.Stuffid)@Html.AutoComplete("Stuffid", "StuffName", "Combo", "StuffInfo", "ID", "StuffName", "IsUsed=True")
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.Supplyid)@Html.AutoComplete("Supplyid", "SupplyName", "Combo", "SupplyInfo", "ID", "ShortName", "SupplyName", "supplykind in ('Su1','Su5') and IsUsed =1 ", null)
                </td>
                <td>@Html.LabelFor(m => m.StartDate)@Html.EditorFor(m => m.StartDate, "DateTimeWithTime")
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.EndDate)@Html.EditorFor(m => m.EndDate, "DateTimeWithTime")
                </td>
                <td>@Html.LabelFor(m => m.IsWhole)@Html.CheckBoxFor(m => m.IsWhole)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.Weight)@Html.EditorFor(m => m.Weight)
                </td>
                <td>@Html.LabelFor(m => m.SumWeight)@Html.EditorFor(m => m.SumWeight)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.InMan)@Html.EditorFor(m => m.InMan)
                </td>
                <td>@Html.LabelFor(m => m.ReportUrl)@Html.EditorFor(m => m.ReportUrl)
                </td>
            </tr>
            <tr>
            <td>@Html.LabelFor(m => m.DocType)@Html.DicEditorFor(m => m.DocType, DicEnum.DocType, new { @data_val_required = "字段是必需的", data_val = "true"})</td>
                <td>@Html.LabelFor(m => m.Remark)@Html.TextAreaFor(m => m.Remark, 3, 30, "")
                </td>
            </tr>
        </table>

}
</div>

<div id="DocTemplateFormDiv" class="mvcform" style="display:none;">
@using (Html.BeginForm())
{@Html.ValidationSummary()
 <table class="mvcform">
            <tr>
            </tr>
            <tr>
                <td>
                    @Html.Label("选择模板:")@Html.DropDownListFor(m => m.ReportType, Enumerable.Empty<SelectListItem>(), new { @data_val_required = "字段是必需的", data_val = "true", zlerp = "combobox" })
                    @*@Html.AutoComplete("LabTemplateID", "LabTemplateName", "Combo", "Lab_Template", "ID", "LabTemplateName", "1=2")*@
                </td>
            </tr>    
 </table>
}
</div>

@*自定义时间*@
<div id="SelectTimeForm" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    { 
        @Html.ValidationSummary()
        <table class="mvcform">
            <tr>
                <td>@Html.Label("起始时间")@Html.Editor("sBeginTime", "DateTimeWithTime")
                </td>
            </tr>
            <tr>
                <td>@Html.Label("终止时间")@Html.Editor("sEndTime", "DateTimeWithTime")
                </td>
            </tr>
            <tr>
                <td>
                    @Html.Label("自动关闭")@Html.CheckBox("sIsAutoClose")
                </td>
            </tr>
        </table>
    }
</div>
