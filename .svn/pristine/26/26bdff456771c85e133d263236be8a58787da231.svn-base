@model ZLERP.Model.CarManage.CarPeccancy
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/CarManage/CarPeccancy.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")'
        };
        $(function () {
            CarPeccancyIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>
<div id="CarPeccancyMyFormDiv" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.ID)
        <table class="mvcform">
            <tr> 
                <td>@Html.LabelFor(m => m.CarID)@Html.DropDownListFor(m => m.CarID, (SelectList)ViewBag.CarList, new { style = "width:140px;" })
                </td>
                <td>@Html.LabelFor(m => m.Driver)@Html.EditorFor(m => m.Driver)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.PeccancyCost)@Html.EditorFor(m => m.PeccancyCost) </td>
                 <td>@Html.LabelFor(m => m.PeccancyDate)@Html.EditorFor(m => m.PeccancyDate)</td>
            </tr> 
              <tr>
                <td>@Html.LabelFor(m => m.PeccancyNum)@Html.EditorFor(m => m.PeccancyNum)</td> 
                <td>@Html.LabelFor(m => m.PeccancyUnit)@Html.DropDownListFor(m => m.PeccancyUnit, (SelectList)ViewBag.CarDealingsList, "", new { style = "width:140px;" })</td>
            </tr>  
            <tr> 
               <td>@Html.LabelFor(m => m.PeccancyItem)@Html.DicEditorFor(m => m.PeccancyItem, DicEnum.PeccancyItem, "") </td>
               <td>@Html.LabelFor(m => m.PeccancyAdress)@Html.EditorFor(m => m.PeccancyAdress)</td>  
            </tr> 
                <tr> 
                  <td colspan=2>@Html.LabelFor(m => m.Remark)@Html.TextAreaFor(m => m.Remark, new { style = "width:75%" })</td> 
            </tr> 
        </table>
    }
</div>
