<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
	<class name="ZLERP.Model.FormulaGHItem, ZLERP.Model" table="FormulaGHItems" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="FormulaGHItemsID">
		 	<generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="StuffAmount" column="StuffAmount" />
		<property name="StandardAmount" column="StandardAmount" />
    <property name="WarpAmount" column="WarpAmount" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />
    <property name="FormulaGHID" column="FormulaGHID"  />
    <property name="StuffTypeID" column="StuffTypeID"  />
		<many-to-one name="StuffType" column="StuffTypeID" class="StuffType" update="false" insert="false" />
    <many-to-one name="FormulaGH" column="FormulaGHID" class="FormulaGH" update="false" insert="false"/>
	</class>
</hibernate-mapping>