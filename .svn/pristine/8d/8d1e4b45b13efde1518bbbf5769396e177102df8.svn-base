    <property name="TranID" column="TranID" />
    <property name="ThatBaleMan" column="ThatBaleMan" />
    <property name="ProjectID" column="ProjectID" />
    <property name="StartDate" column="StartDate" />
    <property name="EndDate" column="EndDate" />
    <property name="BaleType" column="BaleType" />
    <property name="IsOnePrice" column="IsOnePrice" />
    <property name="OnePriceType" column="OnePriceType" />
    <property name="IsStockPrice" column="IsStockPrice" />
    <property name="OnePrice" column="OnePrice" />
    <property name="AllCount" column="AllCount" />
    <property name="AllMoney" column="AllMoney" />
    <property name="AllOkCount" column="AllOkCount" />
    <property name="AllOkMoney" column="AllOkMoney" />
    <property name="PayMoney" column="PayMoney" />
    <property name="PayOwing" column="PayOwing" />
