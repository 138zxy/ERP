      <column name="TranID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
    <many-to-one name="Project"  not-found="ignore">
      <column name="ProjectID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
 
    <many-to-one name="B_FareTemplet"  column="BaleType" class="ZLERP.Model.Beton.B_FareTemplet" update="false" insert="false" not-found="ignore" />
