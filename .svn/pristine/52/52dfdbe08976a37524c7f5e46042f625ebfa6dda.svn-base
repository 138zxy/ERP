    <property name="StockPactID" column="StockPactID" />
    <property name="ThatBaleMan" column="ThatBaleMan" />
    <property name="StuffID" column="StuffID" />
    <property name="StartDate" column="StartDate" />
    <property name="EndDate" column="EndDate" />
    <property name="BaleType" column="BaleType" />
    <property name="InMonth" column="InMonth" />
    <property name="IsOnePrice" column="IsOnePrice" />
    <property name="OnePrice" column="OnePrice" />
    <property name="IsStockPrice" column="IsStockPrice" />
    <property name="AllCount" column="AllCount" />
    <property name="AllMoney" column="AllMoney" />
    <property name="AllOkCount" column="AllOkCount" />
    <property name="AllOkMoney" column="AllOkMoney" />
    <property name="PayMoney" column="PayMoney" />
    <property name="PayOwing" column="PayOwing" />
    <property name="PayFavourable" column="PayFavourable" />
    <property name="BaleDate" column="BaleDate" />
    <property name="BaleMan" column="BaleMan" />
    <property name="Remark" column="Remark" />
    <property name="AuditStatus" column="AuditStatus" />
    <property name="Auditor" column="Auditor" />
    <property name="AuditTime" column="AuditTime" />
    <property name="PiaoPayMoney" column="PiaoPayMoney" />
    <property name="PiaoPayOwing" column="PiaoPayOwing" />
    <property name="PiaoPayFavourable" column="PiaoPayFavourable" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    <many-to-one name="SupplyInfo"  not-found="ignore">
      <column name="StockPactID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
    <many-to-one name="StuffInfo"  not-found="ignore">
      <column name="StuffID" sql-type="nvarchar" not-null="false" />
    </many-to-one> 
  </class>


</hibernate-mapping>