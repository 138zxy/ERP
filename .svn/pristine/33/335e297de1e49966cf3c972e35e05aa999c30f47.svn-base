@model ZLERP.Model.Beton.B_CarFleet
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/Beton/B_CarFleet.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")'
        };
        $(function () {
            B_CarFleetIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>
<div id="MyFormDivB_CarFleet" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.ID)
        <table class="mvcform">
            <tr>
                <td>@Html.LabelFor(m => m.FleetCode)@Html.EditorFor(m => m.FleetCode)
                </td>
                <td>@Html.LabelFor(m => m.FleetName)@Html.EditorFor(m => m.FleetName)
                </td> 
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.FleetType)@Html.EditorFor(m => m.FleetType)
                </td> 
                  <td>@Html.LabelFor(m => m.Adrress)@Html.EditorFor(m => m.Adrress)
                </td>
            </tr> 
             <tr>
              <td>@Html.LabelFor(m => m.Linker)@Html.EditorFor(m => m.Linker)
                </td>
                <td>@Html.LabelFor(m => m.LinkPhone)@Html.EditorFor(m => m.LinkPhone)
                </td>
                
             </tr>
             <tr>
                <td>@Html.LabelFor(m => m.AccountNo)@Html.EditorFor(m => m.AccountNo)
                </td>
                <td>@Html.LabelFor(m => m.TaxNo)@Html.EditorFor(m => m.TaxNo)
                </td>
             </tr>
             <tr>
                <td>@Html.LabelFor(m => m.PaidIn)@Html.TextBoxFor(m => m.PaidIn, new { Value = 0 })
                </td>
                  <td>@Html.LabelFor(m => m.PrepayInit)@Html.TextBoxFor(m => m.PrepayInit, new { Value = 0 })
                </td>
             </tr>  
                 <tr> 
                  <td>@Html.LabelFor(m => m.CarTemplet) @Html.DropDownListFor(m => m.CarTemplet,(IEnumerable<SelectListItem>)ViewBag.CarTemplet,"") 
                </td> 
                 <td>@Html.LabelFor(m => m.IsStop)@Html.EditorFor(m => m.IsStop)
                </td>   
             </tr>  
            <tr>   
            <td>@Html.LabelFor(m => m.Remark)@Html.EditorFor(m => m.Remark)
            </td> 
            </tr>  
        </table>
    }
</div>
