@model ZLERP.Model.CarManage.CarCertificate
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/CarManage/CarCertificate.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")' 
        };
        $(function () {
            CarCertificateIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>
<div id="CarCertificateMyFormDiv" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.ID)
        <table class="mvcform">
            <tr> 
                <td>@Html.LabelFor(m => m.CarID)@Html.DropDownListFor(m => m.CarID, (SelectList)ViewBag.CarList, new { style = "width:140px;" })
                </td>
                <td>@Html.LabelFor(m => m.Name)@Html.EditorFor(m => m.Name)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.CertificateType)@Html.DicEditorFor(m => m.CertificateType, DicEnum.CertificateType, "")</td>
                 <td>@Html.LabelFor(m => m.NextDate)@Html.EditorFor(m => m.NextDate)</td>
            </tr> 
              <tr>
                <td>@Html.LabelFor(m => m.CertificateCost)@Html.EditorFor(m => m.CertificateCost)</td>
                 <td>@Html.LabelFor(m => m.EffectDate)@Html.EditorFor(m => m.EffectDate)</td>
                 
            </tr> 
               <tr>
                <td>@Html.LabelFor(m => m.CertificateNo)@Html.EditorFor(m => m.CertificateNo)</td>
                 <td>@Html.LabelFor(m => m.SignUnit)@Html.DropDownListFor(m => m.SignUnit, (SelectList)ViewBag.CarDealingsList, "", new { style = "width:140px;" })</td> 
            </tr> 
               <tr>
                <td>@Html.LabelFor(m => m.OprateDate)@Html.EditorFor(m => m.OprateDate)</td>
                 <td>@Html.LabelFor(m => m.Oprater)@Html.EditorFor(m => m.Oprater)</td> 
            </tr> 
        <tr> 
                  <td colspan=2>@Html.LabelFor(m => m.Remark)@Html.TextAreaFor(m => m.Remark, new { style = "width:75%" })</td> 
            </tr>  
        </table>
    }
</div>
