<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.Material" assembly="ZLERP.Model">
  <class name="ZLERP.Model.Material.M_BaleBalanceDel, ZLERP.Model" table="M_BaleBalanceDel" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    <id name="ID" column="ID">
      <generator class="native" />
    </id>
    <version name="Version" column="Version" />
    <property name="BaleBalanceID" column="BaleBalanceID" />
    <property name="StuffInID" column="StuffInID" />
    <property name="Price" column="Price" />
    <property name="OtherMoney" column="OtherMoney" />
    <property name="AllMoney" column="AllMoney" /> 
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Remark" column="Remark" />
    <property name="Lifecycle" column="Lifecycle" />
    <many-to-one name="StuffIn"  not-found="ignore">
      <column name="StuffInID" sql-type="nvarchar" not-null="false" />
    </many-to-one>

    <many-to-one name="M_BaleBalance"  not-found="ignore">
      <column name="BaleBalanceID" sql-type="nvarchar" not-null="false" />
    </many-to-one>
  </class>


</hibernate-mapping>