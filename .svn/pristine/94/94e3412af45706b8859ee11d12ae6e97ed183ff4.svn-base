@model ZLERP.Model.SupplyChain.V_SC_YingYS
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/SupplyChain/SC_YingSF.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")',
            DelstoreUrl: '@Url.Action("Find", "SC_VYingYS")',
            UpdateUrl: '@Url.Action("Update", "SC_VYingYS")',
            PayUrl: '@Url.Action("Pay", "SC_YingSF")' 
        };
        $(document).ready(function () {
            $('#SC_PiaoIn_InDate').datepicker(); 
        });
        $(function () {
            SC_YingSFIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>

<div id="myJqGridDetial"> </div> 
 
<div id="SC_YingSFPayForm" class="mvcform" style="display:none;"> 
 
       @Html.LabelFor(m => m.SC_PiaoIn.SC_Supply.SupplierName)@Html.TextBoxFor(m => m.SC_PiaoIn.SC_Supply.SupplierName, new { readOnly = true })  
    @Html.Label("应付金额")@Html.TextBoxFor(m => m.SC_PiaoIn.SC_Supply.PayMoney, new { readOnly = true }) 
    
       <div  id="myJqGridPayDetial"></div>
 
@using (Html.BeginForm())
{@Html.ValidationSummary()
    @Html.HiddenFor(m => m.SC_PiaoIn.SupplierID)
<table class="mvcform"> 
 
    <tr> 
        <td>@Html.LabelFor(m => m.SC_PiaoIn.PayType)@Html.DropDownListFor(m => m.SC_PiaoIn.PayType, (IEnumerable<SelectListItem>)ViewBag.FinanceType, "", new { @class = "text", style = "width:135px;" })</td>
    </tr>
      <tr> 
        <td>@Html.Label("日期")@Html.EditorFor(m => m.SC_PiaoIn.InDate) </td>
        <td>@Html.Label("付款金额")@Html.TextBoxFor(m => m.SC_PiaoIn.PayMoney, new { readOnly = true })</td>
       
    </tr>
    <tr> 
          <td>@Html.LabelFor(m => m.SC_PiaoIn.Payer)@Html.EditorFor(m => m.SC_PiaoIn.Payer)</td> 
        <td>@Html.LabelFor(m => m.SC_PiaoIn.PayFavourable)@Html.TextBoxFor(m => m.SC_PiaoIn.PayFavourable, new { readOnly = true })</td>
     
    </tr>
    <tr> 
           <td>@Html.LabelFor(m => m.SC_PiaoIn.Gatheringer)@Html.EditorFor(m => m.SC_PiaoIn.Gatheringer)</td> 
           <td>@Html.Label("剩余应付额")@Html.TextBoxFor(m => m.SC_PiaoIn.PayOwing, new { readOnly = true })</td>
    </tr>
    <tr> 
        <td colspan="2">@Html.LabelFor(m => m.SC_PiaoIn.Remark2)@Html.TextAreaFor(m => m.SC_PiaoIn.Remark2, new { style = "width:70%" }) </td>
    </tr> 
</table>
}
</div>

<div id="SC_YingSFPayIniForm" class="mvcform" style="display:none;"> 
@using (Html.BeginForm())
{@Html.ValidationSummary()
    @Html.HiddenFor(m => m.SC_YingSFrec.UnitID)
<table class="mvcform"> 
    <tr> 
         <td>@Html.LabelFor(m => m.SC_Supply.SupplierName)@Html.TextBoxFor(m => m.SC_Supply.SupplierName, new { readOnly = true }) </td>
         <td>@Html.Label("原欠款额")@Html.TextBoxFor(m => m.SC_Supply.PayMoney, new { readOnly = true }) </td>
         
    </tr>
      <tr> 
        <td>@Html.LabelFor(m => m.SC_Supply.PaidIn)@Html.TextBoxFor(m => m.SC_Supply.PaidIn, new { readOnly = true }) </td>
         <td>@Html.LabelFor(m => m.SC_Supply.PaidOut)@Html.TextBoxFor(m => m.SC_Supply.PaidOut, new { readOnly = true }) </td>
          
    </tr>
        <tr> 
        <td>@Html.LabelFor(m => m.SC_Supply.PaidFavourable)@Html.TextBoxFor(m => m.SC_Supply.PaidFavourable, new { readOnly = true }) </td>
        <td>@Html.Label("还需应付期初")@Html.TextBoxFor(m => m.SC_Supply.PaidOwing, new { readOnly = true }) </td>
    </tr>
    <tr> <td> <br></br></td></tr>
  
    <tr> 
        <td>@Html.LabelFor(m => m.SC_YingSFrec.FinanceDate)@Html.EditorFor(m => m.SC_YingSFrec.FinanceDate) </td>
          <td>@Html.LabelFor(m => m.SC_YingSFrec.PayType)@Html.DropDownListFor(m => m.SC_YingSFrec.PayType, (IEnumerable<SelectListItem>)ViewBag.FinancePerType, "", new { @class = "text", style = "width:135px;" })</td>
    </tr>
    <tr> 
        <td>@Html.Label("付款金额")@Html.TextBoxFor(m => m.SC_YingSFrec.FinanceMoney) </td>
       <td>@Html.LabelFor(m => m.SC_YingSFrec.PayFavourable)@Html.EditorFor(m => m.SC_YingSFrec.PayFavourable)</td>
    </tr>
    <tr> 
        <td>@Html.LabelFor(m => m.SC_YingSFrec.Payer)@Html.EditorFor(m => m.SC_YingSFrec.Payer)</td> 
        <td>@Html.LabelFor(m => m.SC_YingSFrec.Gatheringer)@Html.EditorFor(m => m.SC_YingSFrec.Gatheringer)</td> 
    </tr>
    <tr> 
        <td colspan="2">@Html.LabelFor(m => m.SC_YingSFrec.Remark)@Html.TextAreaFor(m => m.SC_YingSFrec.Remark, new { style = "width:70%" }) </td>
    </tr> 
</table>
}
</div>