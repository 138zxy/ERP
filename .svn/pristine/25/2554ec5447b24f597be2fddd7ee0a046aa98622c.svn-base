<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.CarManage" assembly="ZLERP.Model">
	<class name="ZLERP.Model.CarManage.CarMaintain, ZLERP.Model" table="CarMaintain" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ID">
      <generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="MaintainNo" column="MaintainNo" />
    <property name="CarID" column="CarID" />
    <property name="RepairWay" column="RepairWay" />
    <property name="RepairDate" column="RepairDate" />
    <property name="RepairMilo" column="RepairMilo" />
    <property name="GiveMan" column="GiveMan" />  
    <property name="RepairUnit" column="RepairUnit" />
    <property name="FaultDesc" column="FaultDesc" />
    <property name="ForecastDate" column="ForecastDate" />
    <property name="GetCarMan" column="GetCarMan" />
    <property name="GetCarDate" column="GetCarDate" />
    <property name="RepairAdress" column="RepairAdress" />
    <property name="IsOver" column="IsOver" />
    <property name="OverDate" column="OverDate" />
    <property name="RepairMan" column="RepairMan" />
    <property name="RepairPirce" column="RepairPirce" />
    <property name="RepairTime" column="RepairTime" />
    <property name="RepairDesc" column="RepairDesc" /> 
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    <many-to-one name="Car"  not-found="ignore">
      <column name="CarID" sql-type="nvarchar" not-null="false"  />
    </many-to-one>
    <many-to-one name="CarDealings"  not-found="ignore">
      <column name="RepairUnit" sql-type="nvarchar" not-null="false"  />
    </many-to-one>
    
  </class>
</hibernate-mapping>