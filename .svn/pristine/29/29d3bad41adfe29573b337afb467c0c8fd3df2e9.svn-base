@model ZLERP.Model.CarManage.CarMaterial
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/CarManage/CarMaterial.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")'
        };
        $(function () {
            CarMaterialIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>
<div id="CarMaterialMyFormDiv" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.ID)
        <table class="mvcform">
            <tr> 
                <td>@Html.LabelFor(m => m.CarID)@Html.DropDownListFor(m => m.CarID, (SelectList)ViewBag.CarList, new { style = "width:140px;" })
                </td>
                <td>@Html.LabelFor(m => m.Oprater)@Html.EditorFor(m => m.Oprater)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.MaterialName)@Html.EditorFor(m => m.MaterialName)</td>
                 <td>@Html.LabelFor(m => m.BuyDate)@Html.EditorFor(m => m.BuyDate)</td>
            </tr> 
              <tr>
                <td>@Html.LabelFor(m => m.Spec)@Html.EditorFor(m => m.Spec)</td>
                 <td>@Html.LabelFor(m => m.Unit)@Html.EditorFor(m => m.Unit)</td> 
            </tr> 
               <tr>
                <td>@Html.LabelFor(m => m.Quantity)@Html.EditorFor(m => m.Quantity)</td>
                 <td>@Html.LabelFor(m => m.Price)@Html.EditorFor(m => m.Price)</td> 
            </tr> 
               <tr>
                <td>@Html.LabelFor(m => m.MoneyAll)@Html.TextBoxFor(m => m.MoneyAll, new { readOnly = true, @class = "inputReadOnly" }) </td>
            </tr> 
            <tr> 
                  <td colspan=2>@Html.LabelFor(m => m.Remark)@Html.TextAreaFor(m => m.Remark, new { style = "width:75%" })</td> 
            </tr> 
        </table>
    }
</div>
