<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model.CarManage" assembly="ZLERP.Model">
	<class name="ZLERP.Model.CarManage.CarPeccancy, ZLERP.Model" table="CarPeccancy" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ID">
      <generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="CarID" column="CarID" />
    <property name="Driver" column="Driver" />
    <property name="PeccancyCost" column="PeccancyCost" />
    <property name="PeccancyDate" column="PeccancyDate" />
    <property name="PeccancyNum" column="PeccancyNum" />
    <property name="PeccancyItem" column="PeccancyItem" />  
    <property name="PeccancyAdress" column="PeccancyAdress" />
    <property name="PeccancyUnit" column="PeccancyUnit" /> 
    <property name="Remark" column="Remark" /> 
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    <many-to-one name="Car"  not-found="ignore">
      <column name="CarID" sql-type="nvarchar" not-null="false"  />
    </many-to-one>
    <many-to-one name="CarDealings"  not-found="ignore">
      <column name="PeccancyUnit" sql-type="nvarchar" not-null="false"  />
    </many-to-one>
    
  </class>
</hibernate-mapping>