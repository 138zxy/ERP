<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
  <class name="ZLERP.Model.SynNC, ZLERP.Model" table="SynNC" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    
        <id name="ID" column="Id">
                <generator class="native" />
        </id>
            <property name="Type" column="Type"  />
            <property name="NO" column="NO"  />
            <property name="IsPositive" column="IsPositive"  />
            <property name="Status" column="Status"  />
            <property name="BuildTime" column="BuildTime" update="false" />
            <property name="SynDate" column="SynDate"  />

    <bag name="SynInfoNCs" batch-size="30" lazy="true" inverse="true" >
      <key column="SynNCID"></key>
      <one-to-many class="SynInfoNC"></one-to-many>
    </bag>
    
    <!--多对多关系暂时不生成-->
    </class>
</hibernate-mapping>