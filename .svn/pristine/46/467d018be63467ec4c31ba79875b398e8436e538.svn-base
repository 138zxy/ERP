<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
  <class name="ZLERP.Model.AlarmLog, ZLERP.Model" table="AlarmLog" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    <id name="ID" column="ID">
      <generator class="native" />
    </id>
    <version name="Version" column="Version" />
    <property name="CarID" column="CarID"/>
    <property name="AlarmTypeID" column="AlarmTypeID"/>
    <property name="DriverName" column="DriverName"/>
    <property name="AlarmData" column="AlarmData"/>
    <property name="AlarmTime" column="AlarmTime"/>
    <property name="Duration" column="Duration"/>
    <property name="AlarmEndTime" column="AlarmEndTime"/>

    <property name="BY1" column="BY1"/>
    <property name="BY2" column="BY2"/>
    <property name="BY3" column="BY3"/>
    <property name="BY4" column="BY4"/>
    <property name="BY5" column="BY5"/>
    <property name="BY6" column="BY6"/>
    <property name="VideoID" column="VideoID"/>
    <property name="Longitude" column="Longitude"/>
    <property name="Latitude" column="Latitude"/>

    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />
    <many-to-one name="Car" column="CarID" class="Car" update="false" insert="false"/>
  </class>
</hibernate-mapping>