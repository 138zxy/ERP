<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
	<class name="ZLERP.Model.View_ContractAll, ZLERP.Model" table="View_ContractAll" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="ContractID">
      <generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="ContractID" column="ContractID" />
    <property name="DataType" column="DataType" />
    <property name="ContractNo" column="ContractNo" />
    <property name="CustName" column="CustName" />
		<property name="ContractName" column="ContractName" />
		<property name="BuildUnit" column="BuildUnit" />
    <property name="ProjectAddr" column="ProjectAddr" />
    <property name="PrePay" column="PrePay" />
    <property name="PaidIn" column="PaidIn" />
		<property name="PaidOut" column="PaidOut" />
		<property name="PaidOwing" column="PaidOwing" />
		<property name="PaidFavourable" column="PaidFavourable" />
		<property name="PayMoney" column="PayMoney" />
    <property name="ContractStatus" column="ContractStatus" />
    
    <property name="Auditor" column="Auditor" />
    <property name="AuditTime" column="AuditTime" /> 
    <property name="Remark" column="Remark" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
   
	</class>
</hibernate-mapping>