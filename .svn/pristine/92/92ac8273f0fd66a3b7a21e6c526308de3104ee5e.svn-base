<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
  <class name="ZLERP.Model.Organization, ZLERP.Model" table="Organization" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
    <id name="ID" column="OrganizationId">
      <generator class="native" />
    </id>
    <version name="Version" column="Version" />
    <property name="OrganizationName" column="OrganizationName" />
    <property name="OrganizationLevel" column="OrganizationLevel" />
    <property name="ParentId" column="ParentId" />
    <property name="IsLeaf" column="IsLeaf" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />

    <many-to-one name="ParentOrganization" column="ParentId" not-found="ignore" class="Organization" update="false" insert="false" />
  </class>

</hibernate-mapping>