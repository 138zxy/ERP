@model ZLERP.Model.Beton.V_B_YingYF
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/Beton/B_YingYF.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")',
            DelstoreUrl: '@Url.Action("Find", "B_VYingSFrec")'
        };
        $(function () {
            B_YingYFIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>

<div id="myJqGridDetial"></div> 
 
 <div id="SC_YingYSPayForm" class="mvcform" style="display:none;"> 
@using (Html.BeginForm())
{@Html.ValidationSummary() 
    @Html.HiddenFor(m => m.B_YingSFrec.UnitID)
<table class="mvcform"> 
    <tr> 
         <td>@Html.Label("运输公司")@Html.TextBoxFor(m => m.Contract.ContractName, new { readOnly = true, @class = "inputReadOnly" }) </td>
         <td>@Html.Label("原欠款额")@Html.TextBoxFor(m => m.Contract.PayMoney, new { readOnly = true, @class = "inputReadOnly" }) </td>
  
    </tr>
    <tr> 
        <td>@Html.LabelFor(m => m.B_YingSFrec.FinanceDate)@Html.EditorFor(m => m.B_YingSFrec.FinanceDate)</td>
        <td>@Html.Label("现有预付金")@Html.TextBoxFor(m => m.Contract.PrePay, new { readOnly = true, @class = "inputReadOnly" }) </td>
 
    </tr>
    <tr> 
        <td>@Html.LabelFor(m => m.B_YingSFrec.PayType)@Html.DropDownListFor(m => m.B_YingSFrec.PayType, (IEnumerable<SelectListItem>)ViewBag.FinanceType, "", new { @class = "text", style = "width:135px;" })</td>
        <td>@Html.Label("预付金额")@Html.EditorFor(m => m.B_YingSFrec.FinanceMoney) </td>
    </tr>
   
    <tr> 
        <td>@Html.LabelFor(m => m.B_YingSFrec.Payer)@Html.EditorFor(m => m.B_YingSFrec.Payer)</td> 
        <td>@Html.LabelFor(m => m.B_YingSFrec.Gatheringer)@Html.EditorFor(m => m.B_YingSFrec.Gatheringer)</td> 
    </tr>
    <tr> 
        <td colspan="2">@Html.LabelFor(m => m.B_YingSFrec.Remark)@Html.TextAreaFor(m => m.B_YingSFrec.Remark, new { style = "width:70%" }) </td>
    </tr> 
</table>
}
</div>