@model ZLERP.Model.App.App_VersionUpdate 
@section HeaderSection{
<script src="@Url.Content("~/Scripts/Views/App/App_VersionUpdate.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        $(function () {
            app_versionupdateIndexInit('@Url.Action("Find")');
        });
</script>
}
<div id="MyJqGrid"></div>
<div id="MyFormDiv" class="mvcform" style="display:none;">
@using (Html.BeginForm())
{@Html.ValidationSummary()
<table class="mvcform"> 
    <tr> 
        <td>@Html.HiddenFor(m=>m.ID)</td>
   
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.AppManageID)@Html.EditorFor(m=>m.AppManageID)</td>
   
    </tr>
    <tr> 
         <td colspan="3">@Html.LabelFor(m => m.UpdateContent)@Html.TextAreaFor(m => m.UpdateContent, 3, 60, new { @class = "text" })
         </td>
    </tr>
    <tr> 
        <td colspan="3">@Html.LabelFor(m => m.UpdateUrl)@Html.TextAreaFor(m => m.UpdateUrl, 1, 60, new { @class = "text" })
         </td>
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.UpdateVersion)@Html.EditorFor(m=>m.UpdateVersion)</td>
   
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.Meno)@Html.EditorFor(m=>m.Meno)</td>
    </tr>
</table>

}
</div>