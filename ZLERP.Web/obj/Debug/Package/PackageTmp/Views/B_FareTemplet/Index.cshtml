@model ZLERP.Model.Beton.V_FareTemplet
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/Beton/B_FareTemplet.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")',
            B_FareTempletDel1Url: '@Url.Action("Find", "B_FareTempletDel1")',
            B_FareTempletDel2Url: '@Url.Action("Find", "B_FareTempletDel2")'
        };
        $(function () {
            B_FareTempletIndexInit(options);
        });
    </script>
}
 
 
<div id="B_FareTemplet" style="float: left; width: 29%;">
</div>

 
<div id="B_FareTempletDel1" style="float: right; width: 70%">
</div>
 
<div id="B_FareTempletDel2" style="float: right; width: 70%"></div>

<div id="MyFormDiv" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.B_FareTemplet.ID)
        <table class="mvcform">
            <tr>
                <td>@Html.LabelFor(m => m.B_FareTemplet.FareTempletName)@Html.EditorFor(m => m.B_FareTemplet.FareTempletName)
                </td> 
            </tr> 
                 <tr>  
                <td>@Html.LabelFor(m => m.B_FareTemplet.Remark)@Html.EditorFor(m => m.B_FareTemplet.Remark)
                </td> 
             </tr>  
        </table>
    }
</div>

<div id="MyFormDiv1" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.B_FareTempletDel1.ID)
         @Html.HiddenFor(m => m.B_FareTempletDel1.FareTempletID)
        <table class="mvcform">
            <tr>
                <td>@Html.LabelFor(m => m.B_FareTempletDel1.FareType)@Html.DicEditorFor(m => m.B_FareTempletDel1.FareType, DicEnum.CarType)
                </td> 
            </tr> 
             <tr>
                <td>@Html.LabelFor(m => m.B_FareTempletDel1.BalaneType)@Html.DropDownListFor(m => m.B_FareTempletDel1.BalaneType, (IEnumerable<SelectListItem>)ViewBag.BalaneType) 
                </td> 
            </tr>  
             <tr>
                <td>@Html.LabelFor(m => m.B_FareTempletDel1.EffectiveTime)@Html.EditorFor(m => m.B_FareTempletDel1.EffectiveTime)
                </td> 
            </tr> 
             <tr>
                <td>@Html.LabelFor(m => m.B_FareTempletDel1.Price)@Html.EditorFor(m => m.B_FareTempletDel1.Price)
                </td> 
            </tr>  
               <tr>
                <td>@Html.LabelFor(m => m.B_FareTempletDel1.CarBetonNum)@Html.EditorFor(m => m.B_FareTempletDel1.CarBetonNum)
                </td> 
            </tr> 
                 <tr>  
                <td>@Html.LabelFor(m => m.B_FareTempletDel1.Remark)@Html.EditorFor(m => m.B_FareTempletDel1.Remark)
                </td> 
             </tr>  
        </table>
    }
</div>
 
<div id="MyFormDiv2" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.B_FareTempletDel2.ID)
         @Html.HiddenFor(m => m.B_FareTempletDel2.FareTempletDelID)
        <table class="mvcform"> 
             <tr>
                <td>@Html.LabelFor(m => m.B_FareTempletDel2.UpKilometre)@Html.EditorFor(m => m.B_FareTempletDel2.UpKilometre)
                </td> 
            </tr> 
                 <tr>
                <td>@Html.LabelFor(m => m.B_FareTempletDel2.DwonKilometre)@Html.EditorFor(m => m.B_FareTempletDel2.DwonKilometre)
                </td> 
            </tr> 
             <tr>
                <td>@Html.LabelFor(m => m.B_FareTempletDel2.Price)@Html.EditorFor(m => m.B_FareTempletDel2.Price)
                </td> 
            </tr> 
                 <tr>  
                <td>@Html.LabelFor(m => m.B_FareTempletDel2.Remark)@Html.EditorFor(m => m.B_FareTempletDel2.Remark)
                </td> 
             </tr>  
        </table>
    }
</div>

