@model ZLERP.Model.HR.V_HR_KQ_Set 
@{var HR_KQ_Set = (ZLERP.Model.HR.HR_KQ_Set)ViewBag.HR_KQ_Set;}
@section HeaderSection{
    
    <script src="@Url.Content("~/Scripts/Views/HR/HR_KQ_Set.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")',
            itemUrl: '@Url.Action("Find", "HR_KQ_SetItem")',
            itemUpdateUrl: '@Url.Action("Update", "HR_KQ_SetItem")',
            AddOrUpdateUrl: '@Url.Action("AddOrUpdate")',
        };
        var model={
                    ID: '@HR_KQ_Set.ID',
                    LeaveMinuteKG: '@HR_KQ_Set.LeaveMinuteKG',
                    LateMinuteKG: '@HR_KQ_Set.LateMinuteKG',
                    KGRate: '@HR_KQ_Set.KGRate',
                    MonthLateMinute: '@HR_KQ_Set.MonthLateMinute',
                    DeviceType: '@HR_KQ_Set.DeviceType',
                    Machine: '@HR_KQ_Set.Machine',
                    Dispatch: '@HR_KQ_Set.Dispatch',
                    IP: '@HR_KQ_Set.IP',
                    SerialPort: '@HR_KQ_Set.SerialPort',
                    BoteLV: '@HR_KQ_Set.BoteLV',
                    PortNo: '@HR_KQ_Set.PortNo',
                    DispatchPass: '@HR_KQ_Set.DispatchPass'
        };
        $(function () {
            init(model);
            HR_KQ_SetIndexInit(options);
        });
    </script>
} 
