@model ZLERP.Model.SupplyChain.SC_PanDian
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/SupplyChain/SC_PanDian.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")',
            DelstoreUrl: '@Url.Action("Find", "SC_PanDianDetail")',
            UpdateUrl: '@Url.Action("Update", "SC_PanDianDetail")',
            PanDianInUrl: '@Url.Action("PanDianIn")'
        };
        $(function () {
            SC_YingSFrecIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid" style="float: left; width: 40%">
</div>

<div id="myJqGridDetial"style="float: right; width:59%">
 <label><input id="IsCheckPandian" type="checkbox" value="" />只显示差异数据</label>
</div> 
<div id="SC_PanDianForm" class="mvcform" style="display:none;">

@using (Html.BeginForm())
{@Html.ValidationSummary()
    @Html.HiddenFor(m => m.ID) 
<table class="mvcform validation-summary-valid"> 
    <tr> 
        <td >@Html.LabelFor(m => m.LibID)@Html.DropDownListFor(m => m.LibID, (IEnumerable<SelectListItem>)ViewBag.Libs, "", new { @class = "text", style = "width:135px;" })</td>
        <td></td>
    </tr> 
    <tr> 
        <td colspan="2">@Html.LabelFor(m => m.Remark)@Html.TextAreaFor(m => m.Remark) </td>
    </tr> 
</table>
}
</div>
 
