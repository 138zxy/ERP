@model ZLERP.Model.CarManage.CarFees
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/CarManage/CarFees.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")'
        };
        $(function () {
            CarFeesIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>
<div id="CarFeesMyFormDiv" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.ID)
        <table class="mvcform">
            <tr> 
                <td>@Html.LabelFor(m => m.CarID)@Html.DropDownListFor(m => m.CarID, (SelectList)ViewBag.CarList, new { style = "width:140px;" })
                </td>
                <td>@Html.LabelFor(m => m.Name)@Html.EditorFor(m => m.Name)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.FeeDate)@Html.EditorFor(m => m.FeeDate) </td>
                 <td>@Html.LabelFor(m => m.FeeCount)@Html.EditorFor(m => m.FeeCount)</td>
            </tr> 
              <tr>
                <td>@Html.LabelFor(m => m.Opreater)@Html.EditorFor(m => m.Opreater)</td>
                 <td>@Html.LabelFor(m => m.FeeType)@Html.EditorFor(m => m.FeeType)</td>
                 
            </tr> 
               <tr>
                <td>@Html.LabelFor(m => m.SignUnit)@Html.DropDownListFor(m => m.SignUnit, (SelectList)ViewBag.CarDealingsList,"", new { style = "width:140px;" })</td>
                 <td>@Html.LabelFor(m => m.NextFeeDate)@Html.EditorFor(m => m.NextFeeDate)</td> 
            </tr>  
            <tr> 
                  <td colspan=2>@Html.LabelFor(m => m.Remark)@Html.TextAreaFor(m => m.Remark, new { style = "width:75%" })</td> 
            </tr> 
        </table>
    }
</div>
