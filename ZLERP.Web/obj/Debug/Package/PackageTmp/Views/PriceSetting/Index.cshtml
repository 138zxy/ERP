    <script src="@Url.Content("~/Scripts/Views/PriceSetting/PriceSetting.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        $(function () {
            priceSettingIndexInit({
                ContractStoreUrl : '@Url.Action("Find", "Contract")',
                ContractItemStoreUrl: '@Url.Action("Find", "ContractItem")',
                ContractItemPriceStoreUrl: '@Url.Action("Find", "PriceSetting")',
                ContractPumpStoreUrl: '@Url.Action("Find", "ContractPump")',
                ContractPumpPriceStoreUrl: '@Url.Action("Find", "ContractPumpPrice")',
                ProjectStoreUrl: '@Url.Action("Find", "Project")'
            });
        });
    </script>
}

