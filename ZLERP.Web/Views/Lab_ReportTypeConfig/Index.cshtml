@model ZLERP.Model.Lab_ReportTypeConfig 
@section HeaderSection{

<script src="@Url.Content("~/Scripts/Views/Lab_ReportTypeConfig/Lab_ReportTypeConfig.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        $(function () {
            lab_reporttypeconfigIndexInit('@Url.Action("Find")');
        });
</script>
}
<div id="MyJqGrid"></div>
<div id="MyFormDiv" class="mvcform" style="display:none;">
@using (Html.BeginForm())
{@Html.ValidationSummary()
<table class="mvcform"> 
    <tr> 
        <td style="display:none;">@Html.LabelFor(m=>m.ID)@Html.EditorFor(m=>m.ID)</td>
    </tr>
    <tr> 
        <td>@Html.LabelFor(m => m.LabReportTypeID)@Html.DropDownListFor(m => m.LabReportTypeID, (IEnumerable<SelectListItem>)ViewBag.LabReportType, new { @data_val_required = "报告类型 字段是必需的", data_val = "true" })
        @*@Html.DicEditorFor(m => m.LabReportTypeID, DicEnum.LabTemplateType,  new { @data_val_required = "字段是必需的", data_val = "true"})*@
        </td>
    </tr>
    <tr> 
        <td>@Html.LabelFor(m => m.StuffTypeID)@Html.DropDownListFor(m => m.StuffTypeID, (IEnumerable<SelectListItem>)ViewBag.StuffType, new { @data_val_required = "材料类型 字段是必需的", data_val = "true" })</td> 
    </tr>
    
</table>

}
</div>
