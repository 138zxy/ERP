@model ZLERP.Model.CarManage.V_CarInsurance
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/CarManage/CarInsurance.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")',
            DelUrl: '@Url.Action("Find","CarInsuranceItem")' 
        };
        $(function () {
            CarInsuranceIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>
<div id="MyJqGridTo2">
</div>
<div id="CarInsuranceMyFormDiv" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.CarInsurance.ID)
        <table class="mvcform">
            <tr> 
                <td>@Html.LabelFor(m => m.CarInsurance.CarID)@Html.DropDownListFor(m => m.CarInsurance.CarID, (SelectList)ViewBag.CarList, new { style = "width:140px;" })
                </td>
                <td>@Html.LabelFor(m => m.CarInsurance.InsuranceType)@Html.DicEditorFor(m => m.CarInsurance.InsuranceType, DicEnum.InsuranceType, "") 
                </td>
            </tr>
            <tr>
              <td>@Html.LabelFor(m => m.CarInsurance.InsuranceUnit)@Html.DropDownListFor(m => m.CarInsurance.InsuranceUnit, (SelectList)ViewBag.CarDealingsList, "", new { style = "width:140px;" })</td>
                
                <td>@Html.LabelFor(m => m.CarInsurance.Linker)@Html.EditorFor(m => m.CarInsurance.Linker) </td>
                 
            </tr> 
              <tr>
                <td>@Html.LabelFor(m => m.CarInsurance.LinkPhone)@Html.EditorFor(m => m.CarInsurance.LinkPhone)</td>
                 <td>@Html.LabelFor(m => m.CarInsurance.InsuranceMan)@Html.EditorFor(m => m.CarInsurance.InsuranceMan)</td>
                 
            </tr> 
               <tr>
               <td>@Html.LabelFor(m => m.CarInsurance.InsuranceNo)@Html.EditorFor(m => m.CarInsurance.InsuranceNo)</td> 
                <td>@Html.LabelFor(m => m.CarInsurance.IsMust)@Html.EditorFor(m => m.CarInsurance.IsMust)</td> 
            </tr> 
             <tr>
               <td>@Html.LabelFor(m => m.CarInsurance.OtherCost)@Html.EditorFor(m => m.CarInsurance.OtherCost)</td> 
                <td>@Html.LabelFor(m => m.CarInsurance.MoneyAll)@Html.EditorFor(m => m.CarInsurance.MoneyAll)</td> 
            </tr>  
        
             <tr>
               <td>@Html.LabelFor(m => m.CarInsurance.StartDate)@Html.EditorFor(m => m.CarInsurance.StartDate)</td> 
                <td>@Html.LabelFor(m => m.CarInsurance.EndDate)@Html.EditorFor(m => m.CarInsurance.EndDate)</td> 
            </tr>  
        
          <tr>
               <td>@Html.LabelFor(m => m.CarInsurance.Opreater)@Html.EditorFor(m => m.CarInsurance.Opreater)</td> 
                <td>@Html.LabelFor(m => m.CarInsurance.OpreateDate)@Html.EditorFor(m => m.CarInsurance.OpreateDate)</td> 
            </tr>   
            <tr> 
                  <td colspan=2>@Html.LabelFor(m => m.CarInsurance.Remark)@Html.TextAreaFor(m => m.CarInsurance.Remark, new { style = "width:75%" })</td> 
            </tr> 
        </table>
    }
</div>



    <div id="CarInsuranceItemForm" class="mvcform" style="display:none;"> 
        @using (Html.BeginForm())
        {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.CarInsuranceItem.ID)
        @Html.HiddenFor(m => m.CarInsuranceItem.CarInsuranceID)
        <table class="mvcform"> 
       
         <tr> 
            <td>@Html.LabelFor(m => m.CarInsuranceItem.CarInsuranceName)@Html.EditorFor(m => m.CarInsuranceItem.CarInsuranceName)</td>   
            
        </tr>
          <tr> 
           <td>@Html.LabelFor(m => m.CarInsuranceItem.ItemMoney)@Html.EditorFor(m => m.CarInsuranceItem.ItemMoney)</td>   
        </tr>
    
        </table>
        }
  </div>
