@*工作性能检测记录*@
<div id="MyFormDiv" class="mvcform" style="display:none;">
@using (Html.BeginForm())
{@Html.ValidationSummary()
<table class="mvcform"> 

    <tr> 
        <td>@Html.Label("试样编号")@Html.TextBoxFor(m => m.ID, new { @readOnly = true })</td>
        <td>@Html.LabelFor(m => m.SamplingDate)@Html.EditorFor(m => m.SamplingDate, "DateTimeWithTime", new { defaultValue = @DateTime.Now.ToString("yyyy-MM-dd hh:mm:ss") })</td>
    </tr>
    <tr> 
        <td>@Html.LabelFor(m => m.ConStrength)@Html.EditorFor(m => m.ConStrength)</td>
        <td>@Html.LabelFor(m => m.ProjectName)@Html.EditorFor(m => m.ProjectName)</td>
    </tr>

    <tr> 
        <td>@Html.LabelFor(m => m.ConsPos)@Html.EditorFor(m => m.ConsPos)</td>
        <td>@Html.LabelFor(m => m.CarNo)@Html.EditorFor(m => m.CarNo)</td>
    </tr>
    <tr> 
        <td>@Html.LabelFor(m => m.ProductLine)@Html.EditorFor(m => m.ProductLine)</td>
        <td>@Html.LabelFor(m => m.Slump)@Html.EditorFor(m => m.Slump)</td>
    </tr>

    <tr> 
        <td>@Html.LabelFor(m => m.ExpansionDegree)@Html.EditorFor(m => m.ExpansionDegree)</td>
        <td>@Html.LabelFor(m => m.WaterRetention)@Html.EditorFor(m => m.WaterRetention)</td>
    </tr>
    <tr> 
        <td>@Html.LabelFor(m => m.Cohesiveness)@Html.EditorFor(m => m.Cohesiveness)</td>
       <td>@Html.LabelFor(m => m.QualifiedJudgment)@Html.EditorFor(m => m.QualifiedJudgment)</td>
    </tr>

    <tr> 
        <td>@Html.LabelFor(m => m.SamplingMan)@Html.EditorFor(m => m.SamplingMan)</td>
        <td>@Html.LabelFor(m => m.Remark)@Html.EditorFor(m => m.Remark)</td>
    </tr>
     <tr> 
        <td>@Html.LabelFor(m => m.ShipDocID)@Html.EditorFor(m => m.ShipDocID)</td>
    </tr>
</table>

}
</div>

@*混凝土实验管理*@
