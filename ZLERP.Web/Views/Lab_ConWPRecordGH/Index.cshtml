    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        <table class="mvcform">
            
                <tr>
                    @Html.HiddenFor(m => m.Lab_ConWPRecordItemsGH.ID)
                    <td style="display:none">
                        @Html.LabelFor(m => m.Lab_ConWPRecordItemsGH.Lab_ConWPRecordGHId)@*@Html.TextBox("Lab_ConWPRecordId")*@
                        @Html.EditorFor(m => m.Lab_ConWPRecordItemsGH.Lab_ConWPRecordGHId)
                        </td>
                    <td>
                    @Html.Label("报告类型:")
