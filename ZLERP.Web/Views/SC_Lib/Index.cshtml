@model ZLERP.Model.SupplyChain.SC_Lib
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/SupplyChain/SC_Lib.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        var options = {
            storeUrl: '@Url.Action("Find")'
        };
        $(function () {
            SC_LibIndexInit(options);
        });
    </script>
}
<div id="MyJqGrid">
</div>
<div id="MyFormDiv" class="mvcform" style="display: none;">
    @using (Html.BeginForm())
    {@Html.ValidationSummary()
        @Html.HiddenFor(m => m.ID )
        <table class="mvcform">
            <tr>
                <td>@Html.LabelFor(m => m.LibName)@Html.EditorFor(m => m.LibName)
                </td>
                <td>@Html.LabelFor(m => m.IsOff)@Html.EditorFor(m => m.IsOff)
                </td>
            </tr>
            <tr> 
                <td>@Html.LabelFor(m => m.Remark)@Html.EditorFor(m => m.Remark)
                </td>
            </tr> 
        </table>
    }
</div>
